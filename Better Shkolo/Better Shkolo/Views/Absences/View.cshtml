@using Better_Shkolo.Models.Absence;
@model IEnumerable<AbsencesesDisplayModel>

<div class="list-group">
    @foreach (var absences in Model)
    {
        <p class="list-group-item list-group-item-action">
            @absences.SubjectName | <a class="btn btn-primary">@absences.Absenceses.Count</a> <a asp-action="Display" asp-controller="Absences" asp-route-subjectId="@absences.SubjectId">View</a>
        </p>
    }
</div>
